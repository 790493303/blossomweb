<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.blossom.web.mapper.LogMapper">

    <!--添加日志信息-->
    <insert id="insertLogInfo" parameterType="com.blossom.web.model.LogModel">
        INSERT INTO
          tb_log(id,logType,httpUrl,httpMethod,description,method,requestIp,exceptionCode,
          exceptionDetail,params,userName,createDate,broswer)
        VALUES (#{id},#{logType},#{httpUrl},#{httpMethod},#{description},#{method},#{requestIp},#{exceptionCode},
                #{exceptionDetail},#{params},#{userName},#{createDate},#{broswer})
    </insert>

</mapper>